{
    "dataname": "depmap23Q2",
    "datasets": {
	"methylation": "methylation.csv",
        "transcriptomics": "depmap23Q2/OmicsExpressionProteinCodingGenesTPMLogp1.csv",
	"proteomics": "proteomics.csv",
        "metabolomics": "metabolomics.csv",
        "drugresponse": "drugresponse.csv",
        "crisprcas9": "depmap23Q2/CRISPRGeneEffect.csv"
    },
    "model": "MOVE",
    "standardize": true,
    "num_epochs": 500,
    "learning_rate": 4e-4,
    "use_conditional": true,
    "batch_size": 100,
    "n_folds": 3,
    "latent_dim": 0.05,
    "hidden_dims": [0.5],
    "probability": 0.4,
    "w_kl": 0.001,
    "views_logits": 256,
    "optimizer_type": "adam",
    "w_decay": 5e-4,
    "reconstruction_loss": "mse",
    "activation_function": "relu",
    "feature_miss_rate_thres": 0.85,
    "label": "tissue",
    "scheduler": "plateau",
    "scheduler_threshold": 1e-4,
    "scheduler_factor": 0.65,
    "scheduler_patience": 20,
    "scheduler_min_lr": 5e-6
}
