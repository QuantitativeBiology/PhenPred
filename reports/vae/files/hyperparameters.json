{
    "dataname": "depmap23Q2",
    "datasets": {
        "methylation": "methylation.csv",
        "transcriptomics": "depmap23Q2/OmicsExpressionProteinCodingGenesTPMLogp1.csv",
	    "proteomics": "proteomics.csv",
        "metabolomics": "metabolomics.csv",
        "drugresponse": "drugresponse.csv",
        "crisprcas9": "depmap23Q2/CRISPRGeneEffect.csv"
    },
    "num_epochs": 150,
    "learning_rate": 1e-3,
    "batch_size": 32,
    "n_folds": 3,
    "latent_dim": 50,
    "hidden_dims": [0.7, 0.4],
    "probability": 0.2,
    "w_kl": 0.01,
    "optimizer_type": "adam",
    "w_decay": 5e-4,
    "reconstruction_loss": "mse",
    "activation_function": "tanh",
    "feature_miss_rate_thres": 0.85,
    "label": "tissue",
    "scheduler": "plateau",
    "scheduler_threshold": 1e-4,
    "scheduler_factor": 0.25,
    "scheduler_patience": 5,
    "scheduler_min_lr": 1e-6
}
