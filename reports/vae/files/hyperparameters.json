{
    "dataname": "depmap23Q2",
    "load_run": null,
    "datasets": {
	    "proteomics": "proteomics.csv",
        "metabolomics": "metabolomics.csv",
        "drugresponse": "drugresponse.csv",
        "crisprcas9": "depmap23Q2/CRISPRGeneEffect.csv",
        "methylation": "methylation.csv",
        "transcriptomics": "depmap23Q2/OmicsExpressionGenesExpectedCountProfileVoom.csv",
        "copynumber": "cnv_summary_20230303_matrix.csv"
    },
    "model": "GMVAE",
    "standardize": true,
    "use_conditional_gmvae": true,
    "labels": ["tissue", "cancer", "mutations", "fussions", "msi", "growth"],
    "filter_features": ["crisprcas9", "transcriptomics", "methylation"],
    "filtered_encoder_only": true,
    "feature_miss_rate_thres": 0.85,
    "num_epochs": 500,
    "learning_rate": 4e-4,
    "batch_size": 100,
    "n_folds": 3,
    "view_latent_dim": 0.05,
    "latent_dim": 50,
    "hidden_dims": [0.7],
    "probability": 0.4,
    "w_kl": 0.001,
    "w_gaussian": 0.01,
    "w_cat": 0.01,
    "views_logits": 256,
    "optimizer_type": "adam",
    "w_decay": 5e-4,
    "reconstruction_loss": "mse",
    "activation_function": "relu",
    "scheduler": "plateau",
    "scheduler_threshold": 1e-4,
    "scheduler_factor": 0.65,
    "scheduler_patience": 7,
    "scheduler_min_lr": 5e-6
}
