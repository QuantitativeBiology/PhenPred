{
    "dataname": "depmap23Q2",
    "datasets": {
        "proteomics": "data/clines//proteomics.csv",
        "metabolomics": "data/clines//metabolomics.csv",
        "drugresponse": "data/clines//drugresponse.csv",
        "crisprcas9": "data/clines//depmap23Q2/CRISPRGeneEffect.csv",
        "methylation": "data/clines//methylation.csv",
        "transcriptomics": "data/clines//depmap23Q2/OmicsExpressionProteinCodingGenesTPMLogp1.csv"
    },
    "model": "GMVAE",
    "standardize": true,
    "num_epochs": 500,
    "learning_rate": 0.0004,
    "use_conditional_gmvae": false,
    "batch_size": 100,
    "n_folds": 3,
    "view_latent_dim": 0.05,
    "latent_dim": 50,
    "hidden_dims": [
        0.7
    ],
    "probability": 0.4,
    "w_kl": 0.001,
    "w_gaussian": 0.01,
    "w_cat": 0.01,
    "views_logits": 256,
    "optimizer_type": "adam",
    "w_decay": 0.0005,
    "reconstruction_loss": "<not serializable>",
    "activation_function": "<not serializable>",
    "feature_miss_rate_thres": 0.85,
    "label": "tissue",
    "scheduler": "plateau",
    "scheduler_threshold": 0.0001,
    "scheduler_factor": 0.65,
    "scheduler_patience": 20,
    "scheduler_min_lr": 5e-06
}